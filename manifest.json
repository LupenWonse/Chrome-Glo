 {
    "name": "Chrome Glo",
    "version": "0.1",
    "description": "Adds context menus for quickly creating cards in Glo Boards from the current page content.",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Login to Glo Boards",
        "default_popup": "login.html"
    },
     "permissions" : ["background","identity","webRequest", "webRequestBlocking", "*://app.gitkraken.com/*","*://api.gitkraken.com/*","tabs","storage","contextMenus"],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
     "key" : "",
     "oauth2" : {
         "client_id" : "645548491999-m3bqfk5r90leo2aakjrkdiesitt36bmi.apps.googleusercontent.com",
         "scopes" : ["email"]
     }
  }